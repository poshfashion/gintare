          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background-color": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".underline": {
              "padding-top": ".5rem",
              ".line": {
                "width": "5rem",
                "height": "1px",
                "display": "inline-block",
                "background-color": "@underlineColor",
                "transition": "width .3s"
              }
            },
            ".title-wrap:hover": {
              ".line": {
                "width": "6rem"
              }
            },
            "& when (@showTitle)": {
              ".title-wrap, .card2 .mbr-card-title": {
                "padding-bottom": "2rem"
              }
            },
            ".card": {
              "height": "100%",
              "padding": "3rem"
            },
            ".card1, .card3": {
              "display": "flex",
              "flex-direction": "column",
              "justify-content": "center"
            },
            ".items-list": {
              "width": "100%",
              ".item-wrap": {
                "display": "flex",
                "justify-content": "space-between",
                "align-items": "center",
                "width": "100%",
                "padding": "1rem 0",
                "border-bottom": "1px solid @linesColor"
              }
            },
            ".mbr-section-btn": {
              "padding-top": "3rem"
            },
            ".card2": {
              "position": "relative"
            },
            ".img-wrap": {
              "position": "absolute",
              "top": "0",
              "left": "0",
              "bottom": "0",
              "right": "0",
              "z-index": "0"
            },
            "amp-img": {
              "height": "100%",
              "img": {
                "object-fit": "cover",
                "object-position": "center center",
                "height": "100%"
              }
            },
            ".wrapper": {
              "z-index": "1",
              "position": "relative",
              "& when (@showSubtitle)": {
                ".mbr-text": {
                  "padding-top": "2rem"
                }
              },
              "& when (@showSubtitle), (@showText)": {
                ".subtext": {
                  "padding-top": "3rem"
                }
              }
            },
            ".mbr-section-subtitle": {
              "color": "#db5761"
            },
            ".subtext": {
              "color": "#db5761"
            },
            ".img-wrap:after": {
              "background": "@imgOverlayColor",
              "opacity": "@imgOverlayOpacity",
              "content": "\"\"",
              "height": "100%",
              "left": "0",
              "position": "absolute",
              "top": "0",
              "width": "100%"
            },
            ".builder-overlay:after": {
              "border-top-left-radius": "30%"
            },
            "@media (max-width: 767px)": {
              ".card": {
                "padding": "2rem 1rem"
              }
            }
